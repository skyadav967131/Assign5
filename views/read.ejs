<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
    <div>
      <a href="/read"><button>Load Data</button></a>
    </div>
    <div>
      <table>
        <thead>
          <tr>
            <% for ( key in jsonData[0] ) { %>
            <th><%= key %></th>
            <% } %>
          </tr>
        </thead>
        <% jsonData.forEach(function(dc) { %>
        <tr>
          <% for ( key in dc ) { %>
          <td><%= dc[key] %></td>
          <% } %>
          <td>
            <a href="/update"><button>Edit</button></a>
            <a href="/delete"><button>Delete</button></a>
          </td>
        </tr>
        <% }) %>
      </table>
    </div>
  </body>
</html>
